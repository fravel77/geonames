    "keywords": [
        "geonames",
        "location",
        "eloquent",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Isern Palaus",
            "email": "ipalaus@ipalaus.com"
        },
        {
            "name": "Arber Dervishi",
            "email": "arberd@gmail.com"
        }        
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-zip": "*",
        "illuminate/config": ">=5.4.0",
        "illuminate/console": ">=5.4.0",
        "illuminate/database": ">=5.4.0",
        "illuminate/filesystem": ">=5.4.0",
        "illuminate/support": ">=5.4.0",
        "symfony/process": "3.2.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.2",
